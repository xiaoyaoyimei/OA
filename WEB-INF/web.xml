<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <description>GetInfomationServlet</description>
    <display-name>GetInfomationServlet</display-name>
    <servlet-name>GetOpenIdServlet</servlet-name>
    <servlet-class>com.weixin.GetOpenIdServlet</servlet-class>
  </servlet>
  <servlet>
    <description>GetInfomationServlet</description>
    <display-name>GetInfomationServlet</display-name>
    <servlet-name>test</servlet-name>
    <servlet-class>com.weixin.test</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>GetOpenIdServlet</servlet-name>
    <url-pattern>/servlet/GetOpenIdServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>test</servlet-name>
    <url-pattern>/servlet/test</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
